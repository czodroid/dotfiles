Host *+l
ProxyCommand ssh -p30022 lartha ncat $(echo %h | sed 's/+.*//') %p

#Host *+a
#ProxyCommand ssh -p22080 root@idefix.ipgp.fr nc $(echo %h | sed 's/+.*//') %p

Host *+a
ProxyCommand ssh -p40022 root@lartha ncat $(echo %h | sed 's/+.*//') %p

Host *+fw
ProxyCommand ssh -p11240 localhost ncat $(echo %h | sed 's/+.*//') %p

Host magdhcp-*
 UserKnownHostsFile /dev/null
 StrictHostKeyChecking no

Host *
 ForwardX11 yes
 NoHostAuthenticationForLocalhost yes
 StrictHostKeyChecking ask
 HashKnownHosts no
 ServerAliveInterval 15

